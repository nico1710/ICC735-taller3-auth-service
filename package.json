		"joi": "^17.9.2",
		"jsonwebtoken": "^9.0.0",
		"mongoose": "^6.7.0",
		"randomatic": "^3.1.1",
		"rut.js": "^2.1.0",
		"swagger-jsdoc": "^6.2.5",
		"swagger-ui-express": "^4.5.0",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
